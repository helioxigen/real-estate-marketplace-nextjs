{
    "name": "rublevka-frontend",
    "version": "1.0.0",
    "main": "index.js",
    "author": "Rublevka",
    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development APP_ENV=development node server/index.js",
        "start": "cross-env NODE_ENV=production APP_ENV=production node server/index.js",
        "build": "cross-env APP_ENV=production NODE_ENV=production next build",
        "run:dev": "cross-env APP_ENV=development NODE_ENV=development next build && cross-env APP_ENV=development NODE_ENV=development node --inspect server/index.js",
        "run:prod": "cross-env APP_ENV=production NODE_ENV=production next build && cross-env APP_ENV=production NODE_ENV=production node --inspect server/index.js",
        "now-build": "APP_ENV=production next build",
        "analyze": "BUNDLE_ANALYZE=both next build",
        "analyze:server": "BUNDLE_ANALYZE=server next build",
        "analyze:browser": "BUNDLE_ANALYZE=browser next build",
        "lint-staged": "lint-staged"
    },
    "dependencies": {
        "@zeit/next-css": "^1.0.1",
        "cacheable-response": "^1.6.12",
        "downshift": "^3.2.12",
        "express": "^4.17.1",
        "fs-extra": "^8.1.0",
        "fuse.js": "^3.4.5",
        "intersection-observer": "^0.7.0",
        "isomorphic-fetch": "^2.2.1",
        "lodash": "^4.17.15",
        "lru-cache": "^5.1.1",
        "lru_map": "^0.3.3",
        "mime": "^2.4.4",
        "next": "^9.0.5",
        "next-compose-plugins": "^2.2.0",
        "next-redux-wrapper": "^3.0.0",
        "next-seo": "^2.0.0",
        "nookies": "^2.0.8",
        "nprogress": "^0.2.0",
        "path-to-regexp": "^3.1.0",
        "performance-now": "^2.1.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-input-mask": "^2.0.4",
        "react-intersection-observer": "^8.24.1",
        "react-is": "^16.9.0",
        "react-modal": "^3.10.1",
        "react-redux": "^7.1.1",
        "react-scroll": "^1.7.14",
        "react-slick": "^0.24.0",
        "react-swipe": "5.1.1",
        "react-yandex-maps": "^4.0.1",
        "redux": "^4.0.4",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0",
        "sharp": "^0.23.0",
        "slick-carousel": "^1.8.1",
        "styled-components": "^4.3.2",
        "swipe-js-iso": "^2.1.5",
        "translitit-cyrillic-russian-to-latin": "^0.1.1",
        "xmlbuilder": "^13.0.2"
    },
    "devDependencies": {
        "@svgr/webpack": "^4.3.2",
        "@zeit/next-bundle-analyzer": "^0.1.2",
        "babel-eslint": "^10.0.3",
        "babel-plugin-styled-components": "^1.10.4",
        "cross-env": "^5.2.1",
        "eslint": "^6.3.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.1.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-webpack": "^0.11.1",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.14.3",
        "husky": "^3.0.5",
        "lint-staged": "^9.2.5",
        "prettier": "^1.18.2",
        "prop-types": "^15.7.2",
        "redux-devtools-extension": "^2.13.8"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ],
        "*.{md,json}": [
            "prettier --write --config .prettierrc",
            "git add"
        ]
    },
    "moduleRoots": [
        "custom-resolver.js"
    ]
}
